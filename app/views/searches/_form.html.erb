<%= form_with(model: search, local: true, class: "form-inline my-2 my-lg-0 mr-auto") do |form| %>
  <%= form.text_field :keywords, class: "form-control mr-sm-2", type: "search", placeholder: "Chercher un ConfianScore" %>
  <%= form.submit "Rechercher", id: "submit-search-btn", class: "btn btn-outline-success my-2 my-sm-0" %>
<% end %>

            
 